#include<stdio.h>
int main()
{
	int r,s1,s2,s3,s4,s5,s6;
	float avg,per;
	char name[20],address[20];
	
	printf("\n Enter roll no ");
	scanf("%d",&r);
	printf("\n Enter name ");
	scanf("%s",name);
	printf("\n Enter address ");
	scanf("%s",address);
	printf("\nEnter marks of each subject ");
	printf("\n Enter c marks ");
	scanf("%d",&s1);
	printf("\n Enter java marks ");
	scanf("%d",&s2);
	printf("\n Enter c# marks ");
	scanf("%d",&s3);
	printf("\n Enter english marks ");
	scanf("%d",&s4);
	printf("\n Enter C++ Programming marks ");
	scanf("%d",&s5);
	printf("\n Enter python Programming marks ");
	scanf("%d",&s6);
	
	
	avg=(s1+s2+s3+s4+s5+s6)/6.0;

	percentage=((s1+s2+s3+s4+s5+s6)/600.0)*100;
	
	printf("\n Details of Student are:");
	printf("\n Roll No = %d",r);
	printf("\n Name = %s",name);
	printf("\n Address = %s",address);
	printf("\n c marks = %d",s1);
	printf("\n java Marks = %d",s2);
	printf("\n c# Marks = %d",s3);
	printf("\n English Marks = %d",s4);
	printf("\n C++ Programming Marks = %d",s5);
	printf("\n python Programming Marks = %d",s6);
	printf("\n Average = %.2f",avg);
	printf("\n Percentage = %.2f  ",per);
	
}
